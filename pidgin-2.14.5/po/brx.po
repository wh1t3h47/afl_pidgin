msgstr "बारायनाय थं"

msgid "Locality"
msgstr "जायगानि"

msgid "Region"
msgstr "ओनसोल"

msgid "Postal Code"
msgstr "पस्टेल क'ड"

msgid "Country"
msgstr "हादोत"

